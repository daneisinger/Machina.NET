      <ELEMENT Tag="Machina/Action/Identity/@Seq" Type="INT" />
      <ELEMENT Tag="Machina/Action/Identity/@Code" Type="INT" />
      <ELEMENT Tag="Machina/Action/Transform" Type="FRAME" />
      <ELEMENT Tag="Machina" Set_Flag="998" />
    </XML>
  </RECEIVE>
  <SEND>
    <XML>
      <ELEMENT Tag="Robot/Ack/@Seq" />
    </XML>
  </SEND>
</ETHERNETKRL>
