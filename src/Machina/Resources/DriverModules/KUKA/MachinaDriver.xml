<?xml version="1.0" encoding="utf-8" ?>
<ETHERNETKRL>
  <CONFIGURATION>
    <EXTERNAL>
      <TYPE>Client</TYPE>
    </EXTERNAL>
    <INTERNAL>
      <ENVIRONMENT>Program</ENVIRONMENT>
      <BUFFERING Mode="FIFO" Limit="16" />
      <BUFFSIZE Limit="65534" />
      <TIMEOUT Connect="65000" />
      <IP>172.31.1.147</IP>
      <PORT>54600</PORT>
      <PROTOCOL>TCP</PROTOCOL>
      <ALIVE Set_Flag="1" />
      <MESSAGES Logging="warning" Display="error" />
